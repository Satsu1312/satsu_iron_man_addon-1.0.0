{
  "name": "Model prime",
  "persistent_data": true,
  "background": "satsu_iron_man_addon:textures/gui/block/background.png",
  "icon": "#satsu_iron_man_addon:armor_icon_ability",
  "gui_display_type": "tree",
  "abilities": {
    "set.scoreboard.armor.60.model_prime": {
      "type": "palladium:command",
      "hidden": true,
      "hidden_in_bar": true,
      "first_tick_commands": [
        "tag @s add satsu.iron.man.suit.set",
        "tag @s add model.prime",
        "scoreboard players set @s satsu.iron.man.armor 60",
        "scoreboard players set @s satsu.iron.man.flame 0",
        "scoreboard players set @s satsu.iron.man.durability 40",
        "scoreboard players set @s satsu.iron.man.flight_speed 64",
        "scoreboard players set @s satsu.iron.man.strength 37"
      ],
      "commands": [""],
      "last_tick_commands": [
        "execute as @s[scores={satsu.iron.man.armor.remote=1}] if entity @e[tag=sentinel.test,distance=0..30] run tag @s add sentinel_one",
        "execute as @s[scores={satsu.iron.man.armor.remote=1}] run function satsu_iron_man_addon:remote_mode/disable",
        "tag @s remove model.prime",
        "tag @s remove satsu.iron.man.suit.set",
        "scoreboard players reset @s satsu.iron.man.animation.armor"
      ],
      "conditions": {
        "enabling": []
      }
    }
  }
}
